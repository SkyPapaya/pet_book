<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.skypapaya.mapper.PetMapper">
    <insert id="insertPet">
        INSERT INTO pet (user_id,name,avatar,species,breed,gender,birthday,health,neuteredï¼Œstatus)
        VALUES(#{userId},#{name},#{avatar},#{species},#{breed},#{gender},#{birthday},#{health},#{neutered},#{status})

    </insert>
    <select id="selectByUserId" resultType="com.skypapaya.vo.PetVO">
        SELECT * FROM pet WHERE user_id = #{userId}
    </select>
</mapper>

